(=== prop08 (k m n) (Nat Nat Nat)
  (sub (add k m) (add k n))
  (sub m n)
)

(=== prop09 (i j k) (Nat Nat Nat)
  (sub (sub i j) k)
  (sub i (add j k))
)

(=== prop10 (m) (Nat)
  (sub m m)
  Z
)

(=== prop17 (n) (Nat)
  (leq n Z)
  (eq n Z)
)

(=== prop18 (i m) (Nat Nat)
  (lt i (S (add i m)))
  True
)

(=== prop21 (n m) (Nat Nat)
  (leq n (add n m))
  True
)

(=== prop22 (a b c) (Nat Nat Nat)
  (max (max a b) c)
  (max a (max b c))
)

(=== prop23 (a b) (Nat Nat)
  (max a b)
  (max b a)
)
